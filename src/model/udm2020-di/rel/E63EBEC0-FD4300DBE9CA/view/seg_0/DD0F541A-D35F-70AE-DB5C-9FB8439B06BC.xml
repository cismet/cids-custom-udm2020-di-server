<userDefinedSQL>CREATE OR REPLACE VIEW TEXTSEARCH&lt;br/&gt;AS &lt;br/&gt;SELECT DISTINCT x.class_id,&lt;br/&gt;                  x.object_id,&lt;br/&gt;                  c.name,&lt;br/&gt;                  To_char(x.string_val)&lt;br/&gt;  FROM   (cs_attr_string x&lt;br/&gt;          left join cs_cat_node c&lt;br/&gt;                 ON (( ( x.class_id = c.class_id )&lt;br/&gt;                       AND ( x.object_id = c.object_id ) )) )&lt;br/&gt;  ORDER  BY x.class_id,&lt;br/&gt;            x.object_id,&lt;br/&gt;            c.name,&lt;br/&gt;            Cast(x.string_val AS VARCHAR2(100))</userDefinedSQL>
